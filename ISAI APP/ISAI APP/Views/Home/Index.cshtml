@{
    ViewBag.Title = "Home Page";
}
<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div class="form-group">
                <label for="txtName">Name:</label>
                <input id="txtName" type="text" class="form-control col-md-12" placeholder="Nombre" title="Nombre" value="Ulises" />
            </div>
            <div class="form-group">
                <label for="txtName">FirstName::</label>
                <input id="txtFirstName" name="firstName" type="text" class="form-control col-md-12" placeholder="Apellido Materno" title="Apellido Paterno" value="Gonzalez" />
            </div>
            <div class="form-group">
                LastName: <input id="txtLastName" name="lastName" type="text" class="form-control col-md-12" placeholder="Apellido Paterno" title="Apellido Materno" value="Juarez" />
            </div>

            <div class="form-group">
                CURP: <input id="txtCURP" name="curp" type="text" class="form-control col-md-12" placeholder="CURP" title="CURP" value="GOJU950519" />
            </div>
            <div class="form-group">
                DateTime: <input id="txtFechaNacimiento" name="fechaNacimiento" type="text" class="form-control col-md-12" title="Fecha de Nacimiento" value="19-05-2019" />
                <div id="Console"></div>
            </div>
            <div class="form-group" >
                <a class="btn btn-success btn-block" href="/FindCropValidate">Siguiente</a>
                @*<button class="btn btn-outline-dark container" type="submit">@Html.ActionLink("Siguiente>>", "Index", "FindCropValidate")</button>*@
            </div>

        </form>
    </div>
</div>
<h2>
    <!--Resultado: name: @ViewBag.name, firstName: @ViewBag.FirstName, lastName: @ViewBag.LastName, CURP: @ViewBag.CURP, fechaNacimiento: @ViewBag.FechaNacimiento-->
</h2>
<h3>@ViewBag.Resultado</h3>
<script>
function AddZero(num) {
    return (num >= 0 && num < 10) ? "0" + num : num + "";
}

window.onload = function() {
    var now = new Date();
    var strDateTime = [[AddZero(now.getDate()),
        AddZero(now.getMonth() + 1),
        now.getFullYear()].join("/"),
        [AddZero(now.getHours()),
        AddZero(now.getMinutes())].join(":"),
        now.getHours() >= 12 ? "PM" : "AM"].join(" ");
    document.getElementById("Console").innerHTML = "Now: " + strDateTime;
    };


    var name = $('#txtName').val();
    var firstName = $('#txtFirstName').val();
    var lastName = $('#txtLastName').val();
    var fechaNacimiento = $('#txtFechaNacimiento').val();
    var curp = $('#txtCURP').val();

    $.ajax({
            url: '@Url.Action("Parametros", "Home")',
        contentType: "application/json; charset=utf-8",
        data: { 'name': name, 'firstName': firstName, 'lastName': lastName, 'fechaNacimiento': fechaNacimiento, 'CURP': curp },
            type: "GET",
            cache: false,
            success: function (response) {
            },
            error: function (er) {
                alert("error");

            }//

        });
</script>