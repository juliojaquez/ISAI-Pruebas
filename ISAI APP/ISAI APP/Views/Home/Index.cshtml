@{
    ViewBag.Title = "Home Page";
}
<form method="post">
    <div class="col-md-4 container">
        <p>Nombre:</p>
        Name: <input id="txtName" name" type="text" class="container" placeholder="Nombre" title="Nombre" value="Ulises"/>
    </div>
    <div class="col-md-4 container">
        <p>Apellido Paterno:</p>
        FirstName: <input id="txtFirstName" name="firstName" type="text" class="container" placeholder="Apellido Materno" title="Apellido Paterno" value="Gonzalez" />
    </div>
    <div class="col-md-4 container">
        <p>Apellido Materno:</p>
        LastName: <input  id="txtLastName" name="lastName" type="text" class="container" placeholder="Apellido Paterno" title="Apellido Materno" value="Juarez" />
    </div>

    <div class="col-md-4 container">
        <p>CURP:</p>
        CURP: <input id="txtCURP" name="curp" type="text" class="container" placeholder="CURP" title="CURP" value="GOJU950519" />
    </div>
    <div class="col-md-4 container">
        <p>Fecha de Nacimiento:</p>
        DateTime: <input id="txtFechaNacimiento" name="fechaNacimiento" type="text" class="container" title="Fecha de Nacimiento" value="19-05-2019"/>
        <div id="Console"></div>
    </div>

    <div class="col-md-4 container">
        <button class="btn btn-default" type="submit" >Obtener información</button>
    </div>
</form>
<h2>
    <!--Resultado: name: @ViewBag.name, firstName: @ViewBag.FirstName, lastName: @ViewBag.LastName, CURP: @ViewBag.CURP, fechaNacimiento: @ViewBag.FechaNacimiento-->
</h2>

<script>
function AddZero(num) {
    return (num >= 0 && num < 10) ? "0" + num : num + "";
}

window.onload = function() {
    var now = new Date();
    var strDateTime = [[AddZero(now.getDate()),
        AddZero(now.getMonth() + 1),
        now.getFullYear()].join("/"),
        [AddZero(now.getHours()),
        AddZero(now.getMinutes())].join(":"),
        now.getHours() >= 12 ? "PM" : "AM"].join(" ");
    document.getElementById("Console").innerHTML = "Now: " + strDateTime;
    };


    var name = $('#txtName').val();
    var firstName = $('#txtFirstName').val();
    var lastName = $('#txtLastName').val();
    var fechaNacimiento = $('#txtFechaNacimiento').val();
    var curp = $('#txtCURP').val();

    $.ajax({            url: '@Url.Action("Parametros", "Home")',        contentType: "application/json; charset=utf-8",        data: { 'name': name, 'firstName': firstName, 'lastName': lastName, 'fechaNacimiento': fechaNacimiento, 'CURP': curp },            type: "GET",            cache: false,            success: function (response) {                alert("Exito");            },            error: function (er) {                alert("error");                       }//        });
</script>